    "# The following program updates a posterior probability distribution using\n",
    "# experimentally obtained data. The example here is a simple coin. The \n",
    "# posterior is the probability that the coin will yield heads. \n",
    "\n",
    "import matplotlib\n",
    "%matplotlib notebook\n",
    "import time\n",
    "\n",
    "# initialize the parameters\n",
    "sample_size = 0;\n",
    "heads_sum = 0;\n",
    "p = np.linspace(0,1,1000);\n",
    "f = (p**heads_sum)*(1-p)**(sample_size - heads_sum);\n",
    "\n",
    "# define the probability distribution plot\n",
    "fig,ax = plt.subplots(1,1)\n",
    "\n",
    "plt.plot(p, f);\n",
    "\n",
    "# iteratively update the likelihood function with new datasets of 10 trials\n",
    "for i in range(0, 50): \n",
    "    f = (p**heads_sum)*(1-p)**(sample_size - heads_sum);\n",
    "    plt.clf();\n",
    "    plt.plot(p, f, linewidth=6.0, color='green')\n",
    "    plt.xlabel('Probability of heads');\n",
    "    plt.ylabel('Prior * Likelihood');\n",
    "    \n",
    "    #redraw the posterior\n",
    "    fig.canvas.draw(),\n",
    "    sys.stdout.write(\"\\rN= %d\" % (i*10))\n",
    "    sys.stdout.flush()\n",
    "    \n",
    "    # Perform 10 more experimental trials. Here, a fair coin is generated. \n",
    "    exp_trials = np.random.binomial(10, 0.5, 1);\n",
    "    \n",
    "    # update the parameters\n",
    "    sample_size += 10;\n",
    "    heads_sum += np.sum(exp_trials);\n",
    "    \n",
    "    # delay before the next iteration\n",
    "    time.sleep(.16);"
   ]
